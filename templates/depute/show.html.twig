{% extends 'base.html.twig' %}

{% block title %}Depute{% endblock %}

{% block body %}
    <h1>Depute</h1>
<div class="row">
<div class="col-md-6">
    <table  class="table table-bordered">
        <tbody class="thead-dark">
            <tr>
                <th>Id</th>
                <td>{{ depute.id }}</td>
            </tr>
             <tr>
                <th>Photo</th>
                <td><img class="img-thumbnail" style="height:100px;width:80px" src="{{ asset('images/deputes/' ~ depute.DepPhoto) }}" ></td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ depute.DepNom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ depute.DepPrenom }}</td>
            </tr>
            <tr>
                <th>Circonscription</th>
                <td>{{ depute.DepCirco }}</td>
            </tr>
            <tr>
                <th>Decision Hcc</th>
                <td>{{ depute.DepDecision }}</td>
            </tr>
            <tr>
                <th>Entite</th>
                <td>{{ depute.DepEntite }}</td>
            </tr>
            <tr>
                <th>Numero Arrete</th>
                <td>{{ depute.DepNumArrete }}</td>
            </tr>
           
        </tbody>
    </table>

    <a href="{{ path('depute_index') }}">Retour Ã  la liste</a> <br/>

   
    
  </div>
</div>

  




{% endblock %}
